<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="room" nsURI="http://room/1.0" nsPrefix="room">
  <eClassifiers xsi:type="ecore:EClass" name="Room">
    <eOperations name="calculateSquareMeters"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="corners" lowerBound="3"
        upperBound="-1" eType="#//Corner" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="squaremeters" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="height" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Corner">
    <eStructuralFeatures xsi:type="ecore:EReference" name="coordinates" lowerBound="1"
        eType="ecore:EClass location.ecore#//GeoCoordinates"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="previousCorner" lowerBound="1"
        eType="#//Corner"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="nextCorner" lowerBound="1"
        eType="#//Corner"/>
  </eClassifiers>
</ecore:EPackage>
